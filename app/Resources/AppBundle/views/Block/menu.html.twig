                        {% endfor %}
                    </ul>
                </div>
            </div>
        {% endif %}
        {% if secondary|length %}
            <div class="header-menu-block topmenu">
                <div class="type">
                    <div class="type-title">{{ 'frontend.menu.accessories'|trans([], 'AppBundle') }}</div>

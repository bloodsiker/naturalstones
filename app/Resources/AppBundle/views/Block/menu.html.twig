                        {% for sCategory in secondary %}
                            <li><a href="{{ path('product_list', { 'slug': sCategory.slug }) }}">{{ sCategory.getName}}</a></li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        {% endif %}
        {% if giftBox|length %}
            <div class="header-menu-block topmenu">
                <div class="type">

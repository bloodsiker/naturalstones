
{% if (settings['type'] == 'header') %}
    <div class="header-menu-outer header-menu-desctop">
        <div class="title">{{ 'frontend.menu_title'|trans([], 'AppBundle') }}</div>
        {% if individual|length %}
            <div class="header-menu-block topmenu">
                <div class="type">
                    <div class="type-title">{{ 'frontend.menu.individual'|trans([], 'AppBundle') }}</div>
                    <ul class="header-menu submenu">
                        {% for iCategory in individual %}
                            <li><a href="{{ path('product_list', { 'slug': iCategory.slug }) }}">{{ iCategory.getName()}}</a></li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        {% endif %}
        {% if gematit|length %}
            <div class="header-menu-block topmenu">
                <div class="type">
                    <div class="type-title">{{ 'frontend.menu.gematit'|trans([], 'AppBundle') }}</div>
                    <ul class="header-menu submenu">
                        {% for gCategory in gematit %}
                            <li><a href="{{ path('product_list', { 'slug': gCategory.slug }) }}">{{ gCategory.getName}}</a></li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        {% endif %}
        {% if main|length %}
            <div class="header-menu-block topmenu">
                <div class="type">
                    <div class="type-title">{{ 'frontend.menu.decorations'|trans([], 'AppBundle') }}</div>
